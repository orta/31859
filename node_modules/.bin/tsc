#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/ortatherox/dev/typescript/repros/31859/node_modules/.pnpm/typescript@3.9.6/node_modules/typescript/bin/tsc/node_modules:/Users/ortatherox/dev/typescript/repros/31859/node_modules/.pnpm/typescript@3.9.6/node_modules/typescript/bin/node_modules:/Users/ortatherox/dev/typescript/repros/31859/node_modules/.pnpm/typescript@3.9.6/node_modules/typescript/node_modules:/Users/ortatherox/dev/typescript/repros/31859/node_modules/.pnpm/typescript@3.9.6/node_modules:/Users/ortatherox/dev/typescript/repros/31859/node_modules/.pnpm/node_modules:/Users/ortatherox/dev/typescript/repros/31859/node_modules:/Users/ortatherox/dev/typescript/repros/node_modules:/Users/ortatherox/dev/typescript/node_modules:/Users/ortatherox/dev/node_modules:/Users/ortatherox/node_modules:/Users/node_modules:/node_modules:/Users/ortatherox/dev/typescript/repros/31859/node_modules/typescript/bin/tsc/node_modules:/Users/ortatherox/dev/typescript/repros/31859/node_modules/typescript/bin/node_modules:/Users/ortatherox/dev/typescript/repros/31859/node_modules/typescript/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typescript/bin/tsc" "$@"
else 
  exec node  "$basedir/../typescript/bin/tsc" "$@"
fi

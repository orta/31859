#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/ortatherox/dev/typescript/repros/31859/node_modules/.pnpm/loose-envify@1.4.0/node_modules/loose-envify/cli.js/node_modules:/Users/ortatherox/dev/typescript/repros/31859/node_modules/.pnpm/loose-envify@1.4.0/node_modules/loose-envify/node_modules:/Users/ortatherox/dev/typescript/repros/31859/node_modules/.pnpm/loose-envify@1.4.0/node_modules:/Users/ortatherox/dev/typescript/repros/31859/node_modules/.pnpm/node_modules:/Users/ortatherox/dev/typescript/repros/31859/node_modules:/Users/ortatherox/dev/typescript/repros/node_modules:/Users/ortatherox/dev/typescript/node_modules:/Users/ortatherox/dev/node_modules:/Users/ortatherox/node_modules:/Users/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../loose-envify@1.4.0/node_modules/loose-envify/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../loose-envify@1.4.0/node_modules/loose-envify/cli.js" "$@"
fi
